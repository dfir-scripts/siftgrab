    echo -e "**  11)${GREEN} EventTranscriptParser.py (Parse Eventranscript.db)${NORMAL}"
    echo ""
    echo -e "Select a submenu option number or ${RED}enter to go back. ${NORMAL}"
    read subopt
    while [ subopt != '' ]
    do
        if [[ $subopt = "" ]]; then
